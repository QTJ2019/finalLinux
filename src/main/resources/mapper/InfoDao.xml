<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jian.finallinux.dao.InfoDao">
    <select id="getPerson" resultType="com.jian.finallinux.entity.Person">
        select person_name, person_number
        from  tb_person
        where person_id=#{id}
    </select>


</mapper>